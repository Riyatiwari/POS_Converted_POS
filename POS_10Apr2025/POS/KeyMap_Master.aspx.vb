    Public Sub btn_Ok_Click(sender As Object, e As EventArgs)
        Try

            Dim pName As String = ""
            Dim pID As Integer = 0
            Dim sName As String = ""
            Dim sizeID As Integer = 0
            Dim pCount As Integer = 0

            For Each item As RepeaterItem In rdProduct_list.Items

                Dim hdProductID As HiddenField = item.FindControl("hdProductID")
                Dim hdProductName As HiddenField = item.FindControl("hdProductName")
                Dim chk_productID As WebControls.CheckBox = item.FindControl("chk_productID")
                Dim ddlSize As WebControls.DropDownList = item.FindControl("ddlSize")

                If chk_productID.Checked = True Then
                    pCount = pCount + 1

                    If pCount = 1 And pName = "" Then
                        pName = hdProductName.Value.ToString()
                        pID = Val(hdProductID.Value.ToString())
                        sizeID = Val(ddlSize.SelectedValue)
                        sName = ddlSize.SelectedItem.Text
                    Else
                        pName = ""
                        pID = 0
                        sizeID = 0
                        sName = ""
                        ddlGroupCategory.ClearSelection()
                        ddlProductGroup.ClearSelection()
                        radBackColor.SelectedColor = System.Drawing.ColorTranslator.FromHtml("#1B7BBD")
                        radFontColor.SelectedColor = ColorTranslator.FromHtml("#FFFFFF")
                        ScriptManager.RegisterStartupScript(Me, Me.GetType, "test", "alert('Plese Select only one Product.'); $('#Psummary thead tr').clone(true).appendTo('#Psummary thead'); Grid(); popup();", True)


                        Exit For

                    End If
                End If

            Next

            '----------------Set detail as btn text----------------
            ReBindGrid()
            Dim str As String = ""

