    Public Sub bindTillList()
        Try
            oclsKeyMap.cmp_id = Val(Session("cmp_id"))
            Dim dtTills As DataTable = oclsKeyMap.SelectTills()
            If dtTills.Rows.Count > 0 Then
                rdTill.DataSource = dtTills
                rdTill.DataBind()
            Else
                rdTill.DataSource = Nothing
                rdTill.DataBind()
            End If
        Catch ex As Exception
            LogHelper.Error("Key_Map_Master:bindTillList" + ex.Message)
        End Try
    End Sub
    Private Sub bindSortingNo()
