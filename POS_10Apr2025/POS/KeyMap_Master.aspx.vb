    Protected Sub SetButton(buttonid As String)
        Try
            hfRowIdbutton.Value = buttonid.ToString()
            lblRowid.Text = buttonid.ToString()

            If Session("CopyMode").ToString() = "1" And ViewState("CopyModefromButtonId").ToString() <> "" Then
                CopyButton(ViewState("CopyModefromButtonId"), buttonid)
            ElseIf Session("CopyMode").ToString() = "1" Then
                ViewState("CopyModefromButtonId") = buttonid.ToString()
            End If
        Catch ex As Exception
            LogHelper.Error("Key_Map_Master:SetButton :" + ex.Message)
        End Try
    End Sub

