    Private Sub BindData(ByVal id As String)
        Try

            Dim Data_dt As DataTable = DirectCast(ViewState("Data_dt"), DataTable)

            For Each row As DataRow In Data_dt.Rows
                If row("id").ToString() = id.ToString() Then
                    hfRowIdbutton.Value = row("id").ToString()
                    lblRowid.Text = row("id").ToString()
                    '-------------------Old ---------------
                    ddlMainCategory.SelectedValue = Val(row("maincategory_id"))

                    RadPro_group.SelectedValue = Val(row("Product_Group_id"))

                    oclsBind.BindProductByProductGroupLocation(Radproduct, Val(Session("cmp_id")), RadPro_group.SelectedValue, Val(IIf(radLocation.SelectedValue = "", 0, radLocation.SelectedValue)))
                    Radproduct.SelectedValue = Val(row("Product_id"))

                    oclsBind.BindSizeUnitByProductandLocation(RadSize, Val(Session("cmp_id")), Radproduct.SelectedValue, Val(IIf(radLocation.SelectedValue = "", 0, radLocation.SelectedValue)))
                    RadSize.SelectedValue = Val(row("Size_id"))

                    radBackcolorpicker.SelectedColor = ColorTranslator.FromHtml(row("BackColor"))
                    radForcolorpicker.SelectedColor = ColorTranslator.FromHtml(row("ForeColor"))

                    '--------------------New ------ bind popup controlls------------------------------------

                    Try
                        oclsBind.BindProductGroupMain(ddlGroupCategory, Val(Session("cmp_id")))
                        oclsBind.BindProductGroup(ddlProductGroup, Val(Session("cmp_id")))

                        ddlGroupCategory.SelectedValue = row("maincategory_id")
                        ddlProductGroup.SelectedValue = row("Product_Group_id")
                    Catch ex As Exception

                    End Try

                    'ddlProductGroup.Items.FindByValue(row("Product_Group_id")).Selected = True
                    Try
                        ddlPrdGfilter.SelectedValue = row("Product_Group_id")
                        'ddlPrdGfilter.Items.FindByValue().Selected = True
                    Catch ex As Exception

                    End Try
                    ReBindGrid()
                    For Each item As RepeaterItem In rdProduct_list.Items

                        Dim hdProductID As HiddenField = item.FindControl("hdProductID")
                        Dim hdProductName As HiddenField = item.FindControl("hdProductName")
                        Dim chk_productID As WebControls.RadioButton = item.FindControl("chk_productID")
                        Dim ddlSize As WebControls.DropDownList = item.FindControl("ddlSize")
                        Dim hdCateogoryname As HiddenField = item.FindControl("hdCateogoryname")



                        If Val(hdProductID.Value.ToString()) = Val(row("Product_id")) Then
                            oclsBind.BindSizeUnitByProductandLocation(ddlSize, Val(Session("cmp_id")), Val(hdProductID.Value.ToString()), Val(IIf(radLocation.SelectedValue = "", 0, radLocation.SelectedValue)))
                            hdProductNameSelected.Value = hdCateogoryname.Value



                            chk_productID.Checked = True
                            ddlSize.SelectedValue = Val(row("Size_id"))

                        Else
                            chk_productID.Checked = False
                        End If

                    Next

                    radFontColor.SelectedColor = ColorTranslator.FromHtml(row("ForeColor"))
                    radBackColor.SelectedColor = ColorTranslator.FromHtml(row("BackColor"))

                    Exit For
                End If
            Next

        Catch ex As Exception
            ScriptManager.RegisterStartupScript(Me, Me.GetType, "test", "alert('" + ex.Message.ToString().Replace("'", "") + "');", True)
            LogHelper.Error("Key_Map_Master:BindData:" + ex.Message)
        End Try
    End Sub

