    Protected Sub radVenue_SelectedIndexChanged(sender As Object, e As RadComboBoxSelectedIndexChangedEventArgs)
        Try
            If radVenue.SelectedIndex = 0 Then
                radLocation.Items.Clear()
                radMachine.Items.Clear()
                oclsBind.BindLocationByVenue(radLocation, Val(Session("cmp_id")), 0)
            Else
                oclsBind.BindLocationByVenue(radLocation, Val(Session("cmp_id")), Val(radVenue.SelectedValue))
                radMachine.Items.Clear()
            End If

        Catch ex As Exception
            LogHelper.Error("Key_Map_Master:radVenue_SelectedIndexChanged:" + ex.Message)
        End Try
    End Sub
    Protected Sub radLocation_SelectedIndexChanged(sender As Object, e As RadComboBoxSelectedIndexChangedEventArgs)
        Try
            If radLocation.SelectedIndex = 0 Then
                radMachine.Items.Clear()
                Radproduct.Items.Clear()
                oclsBind.BindProductByProductGroupLocation(Radproduct, Val(Session("cmp_id")), RadPro_group.SelectedValue, Val(IIf(radLocation.SelectedValue = "", 0, radLocation.SelectedValue)))
                RadSize.Items.Clear()
                oclsBind.BindSizeUnitByProductandLocation(RadSize, Val(Session("cmp_id")), Radproduct.SelectedValue, Val(IIf(radLocation.SelectedValue = "", 0, radLocation.SelectedValue)))
            Else
                oclsBind.BindMachineByLocation(radMachine, Val(Session("cmp_id")), Val(radLocation.SelectedValue))
                Radproduct.Items.Clear()
                oclsBind.BindProductByProductGroupLocation(Radproduct, Val(Session("cmp_id")), RadPro_group.SelectedValue, Val(IIf(radLocation.SelectedValue = "", 0, radLocation.SelectedValue)))
                RadSize.Items.Clear()
