                div_imgOption.Visible = True

            Else
                div_imgOption.Visible = False
                ddl_ImageOption.ClearSelection()
            End If

        Catch ex As Exception
            LogHelper.Error("Key_Map_Master:ddlImgType_SelectedIndexChanged" + ex.Message)
        End Try
