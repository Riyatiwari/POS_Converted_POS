    Protected Sub Page_Load(sender As Object, e As EventArgs) Handles Me.Load
        Try

            LogHelper.Error("Page_Load started.")
            If Not IsPostBack Then
                LogHelper.Error("First-time page load detected.")
                If Session("cmp_id") = Nothing Then
                    Response.Redirect("SignIn.aspx", False)
                End If

                If Not Session("key_map_id") = Nothing Then
                    LogHelper.Error("Session 'key_map_id' found. Calling BindKeyMap.")
                    BindKeyMap()
                Else
                    bindSortingNo()
                    oclsBind.BindVenue(radVenue, Val(Session("cmp_id")))
                    radBackcolorpicker_m.SelectedColor = System.Drawing.ColorTranslator.FromHtml("#1B7BBD")
                End If
                LogHelper.Error("Binding product groups for cmp_id: " & Val(Session("cmp_id")).ToString())
                oclsBind.BindProductGroupMain(ddlMainCategory, Val(Session("cmp_id")))
                oclsBind.BindProductGroup(RadPro_group, Val(Session("cmp_id")))
                radBackcolorpicker.SelectedColor = System.Drawing.ColorTranslator.FromHtml("#1B7BBD")

            End If

        Catch ex As Exception
            LogHelper.Error("Key_Map_Master:Page_Load" + ex.Message)
        End Try
    End Sub

    Private Sub bindSortingNo()
        Try
            oclsKeyMap.cmp_id = Val(Session("cmp_id"))
            oclsKeyMap.form_name = "Key_Map"
            Dim dt As DataTable = oclsKeyMap.Select1()
            If dt.Rows.Count > 0 Then
                txtSortingno.Text = Val(dt.Rows(0)("SNO"))
            End If
        Catch ex As Exception
            LogHelper.Error("Key_Map_Master:bindSortingNo" + ex.Message)
        End Try
    End Sub
