    Private Sub Insert_KeyMapDetails(ByVal id As Integer)
        Try

            oclsKeyMap.key_map_id = id
            oclsKeyMap.cmp_id = Val(Session("cmp_id"))
            oclsKeyMap.Delete_Edit()

            Dim Data_dt As DataTable = DirectCast(ViewState("Data_dt"), DataTable)

            For Each row As DataRow In Data_dt.Rows
                If Val(row("Product_Group_id")) > 0 Then
                    oclsKeyMap.key_map_id = id
                    oclsKeyMap.Product_Group_id = Val(row("Product_Group_id"))
                    oclsKeyMap.Product_id = Val(row("Product_id"))
                    oclsKeyMap.Size_id = Val(row("Size_id"))
                    oclsKeyMap.cmp_id = Val(Session("cmp_id"))
                    oclsKeyMap.ip_address = Request.UserHostAddress
                    oclsKeyMap.login_id = Val(Session("login_id"))
                    oclsKeyMap.is_active = 1
                    oclsKeyMap.BG_Color = row("BackColor")
                    oclsKeyMap.FG_Color = row("ForeColor")
                    oclsKeyMap.matrix = row("matrix")
                    oclsKeyMap.maincategory_id = Val(row("maincategory_id"))
                    oclsKeyMap.Insert_KeyMapDetails()
                End If
            Next

        Catch ex As Exception
            ScriptManager.RegisterStartupScript(Me, Me.GetType, "test", "alert('" + ex.Message.ToString().Replace("'", "") + "');", True)
            LogHelper.Error("Promotion:btnSave_Click:" + ex.Message)
